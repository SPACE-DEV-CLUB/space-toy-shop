{
  "packageName": "styled-component",
  "name": "cyclus-styled-component",
  "description": "Generates styled-component snippets from colors, text styles and layers.",
  "version": "2.7.0",
  "author": {
    "name": "Mortz Gruber"
  },
  "options": [
    {
      "name": "Color format",
      "type": "picker",
      "id": "colorFormat",
      "options": [
        {
          "name": "Default",
          "value": "default"
        },
        {
          "name": "HEX",
          "value": "hex"
        },
        {
          "name": "RGB",
          "value": "rgb"
        },
        {
          "name": "HSL",
          "value": "hsl"
        }
      ],
      "default": "default"
    },
    {
      "name": "Dimensions",
      "type": "switch",
      "id": "showDimensions",
      "default": true
    },
    {
      "name": "Unitless line-height",
      "type": "switch",
      "id": "unitlessLineHeight",
      "default": true
    },
    {
      "name": "Text styles as variables",
      "type": "switch",
      "id": "mixin",
      "default": true
    },
    {
      "name": "Default values",
      "type": "switch",
      "id": "showDefaultValues",
      "default": true
    },
    {
      "name": "Display resources from linked and parent styleguides",
      "type": "switch",
      "id": "useLinkedStyleguides",
      "default": true
    },
    {
      "name": "Use “rem” unit",
      "type": "switch",
      "id": "useRemUnit",
      "default": true
    },
    {
      "name": "Show padding, margin estimations (experimental)",
      "type": "switch",
      "id": "showPaddingMargin",
      "default": true
    }
  ],
  "platforms": [
    "web"
  ],
  "moduleURL": "./main.ab918c0d.js",
  "repository": "https://github.com/zeplin/stylesheet-extensions.git",
  "readmeURL": "./README.md"
}